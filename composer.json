{
    "name": "sunaoka/holidays",
    "description": "Holiday calculation library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sunaoka",
            "email": "sunaoka@pocari.org"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "composer/composer": "^1.0 || ^2.0",
        "nesbot/carbon": "^1.0 || ^2.0",
        "phpunit/phpunit": ">=5.0",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Sunaoka\\Holidays\\": ["bin/", "src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sunaoka\\Holidays\\Tests\\": "./tests/"
        }
    },
    "bin": [
        "bin/holiday-update"
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
